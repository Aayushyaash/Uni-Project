@article{ WOS:000911316000024,
Author = {Bates, Sarah and Reeve, Belinda and Trevena, Helen},
Title = {A narrative review of online food delivery in Australia: challenges and
   opportunities for public health nutrition policy},
Journal = {PUBLIC HEALTH NUTRITION},
