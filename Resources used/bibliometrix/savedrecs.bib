@inproceedings{ WOS:000986953900064,
Author = {Huq, Farhana and Sultana, Nahar and Razzaque, Md. Abdur},
Book-Group-Author = {IEEE},
Title = {Quality of Service Aware Order Allocation for Inter-Regional Online Food
   Delivery Systems},
