@article{ WOS:000780010200003,
Author = {Bang Nguyen and Nguyen, Van-Ho and Thanh Ho},
Title = {Sentiment Analysis of Customer Feedback in Online Food Ordering Services},
Journal = {BUSINESS SYSTEMS RESEARCH JOURNAL},
