@inproceedings{ WOS:000426436200065,
Author = {Riskinanto, Anggar and Kelana, Bayu and Hilmawan, Deliar Rifda},
Editor = {Lwin, KT},
Title = {The Moderation Effect of Age on Adopting E-Payment Technology},
Booktitle = {4TH INFORMATION SYSTEMS INTERNATIONAL CONFERENCE (ISICO 2017)},
Series = {Procedia Computer Science},
Year = {2017},
Volume = {124},
Pages = {536-543},
Note = {4th Information Systems International Conference (ISICO) - Innovation of
   Information Systems - Visions, Opportunities and Challenges, Bali,
   INDONESIA, NOV 06-08, 2017},
Organization = {Inst Teknologi Sepuluh Nopember, Dept Informat Syst},
Abstract = {E-payment is one of the fin-tech solutions that has gained users from
   different generations. This is unique as it is known that distinctive
   generations may adopt technology differently. This study aimed to assess
   on how the adoption of an e-payment system differs on different
   generations using the quantitative method. We used TAM model moderated
   by age as the research model. Data was collected through an online
   questionnaire and successfully gathered 532 responses from application
   users. The findings showed that only perceived ease of use has a
   positive effect on perceived of usefulness, that was moderated by age.
   This result may provide a new perspective on how e-payment being adopted
   on Indonesian users. (c) 2017 The Authors. Published by Elsevier B.V.},
